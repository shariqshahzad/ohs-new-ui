{
  "name": "dashboard-spa-ohs",
  "version": "0.1.0",
  "description": "Boilerplate for starting new React projects for Qiwa",
  "main": "index.js",
  "scripts": {
    "build": "tsc --pretty --noEmit && webpack --config scripts/build.ts --node-env=production",
    "start": "webpack serve --config scripts/start.ts --node-env=development",
    "start:mock": "cross-env APP_API_MOCK=true HTTPS=false npm start",
    "test:unit": "cross-env NODE_ICU_DATA=node_modules/full-icu node --no-compilation-cache -r ts-node/register scripts/test.ts --maxWorkers=7 --passWithNoTests",
    "coverage": "npm run test:unit -- --coverage",
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "prepare": "npx husky install",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "type-check": "tsc --pretty --noEmit",
    "pre-commit": "lint-staged",
    "generator": "plop",
    "translations": "ts-node -T --skip-project ./scripts/translations/fetch-translations.ts"
  },
  "engines": {
    "node": "18"
  },
  "lint-staged": {
    "!(*config).{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx}": [
      "bash -c 'npm run type-check'"
    ],
    "*.{json}": [
      "prettier --write"
    ],
    "*.{css,pcss,scss}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Qiwa",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@svgr/webpack": "7.0.0",
    "@testing-library/jest-dom": "5.16.5",
